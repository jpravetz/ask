{
  "name": "@jpravetz/ask",
  "version": "2.0.3-beta.10",
  "exports": {
    ".": "./mod.ts"
  },
  "tasks": {
    "dev": "deno run --watch main.ts",
    "check": "deno check mod.ts lib examples"
  },
  "fmt": {
    "singleQuote": true,
    "lineWidth": 110,
    "include": [
      "lib",
      "examples",
      "launch.config.json",
      "mod.ts"
    ]
  },
  "lint": {
    "include": [
      "lib",
      "examples",
      "mod.ts"
    ]
  },
  "imports": {
    "$types": "./lib/types.ts",
    "$item": "./lib/item/mod.ts",
    "$opts": "./lib/opts/mod.ts",
    "$io": "./lib/io/mod.ts",
    "$prompt": "./lib/prompt/mod.ts",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/io": "jsr:@std/io@^0.225.0"
  }
}