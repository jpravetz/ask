{
  "name": "@jpravetz/ask",
  "version": "2.0.3-beta.24",
  "exports": {
    ".": "./mod.ts"
  },
  "tasks": {
    "dev": "deno run --watch main.ts",
    "check": "deno check mod.ts lib examples",
    "test": "deno run -A ./examples/test.ts"
  },
  "fmt": {
    "singleQuote": true,
    "lineWidth": 110,
    "include": [
      "lib",
      "examples",
      "launch.config.json",
      "mod.ts"
    ]
  },
  "lint": {
    "include": [
      "lib",
      "examples",
      "mod.ts"
    ]
  },
  "imports": {
    "$errors": "./lib/errors.ts",
    "$fmt": "./lib/format.ts",
    "$types": "./lib/types.ts",
    "$list": "./lib/list/mod.ts",
    "$opts": "./lib/opts/mod.ts",
    "$io": "./lib/io/mod.ts",
    "$prompt": "./lib/prompt/mod.ts",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/io": "jsr:@std/io@^0.225.2"
  }
}